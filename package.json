{
  "name": "search-it",
  "version": "1.0.0",
  "description": "IT Job search",
  "main": "dist/back/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/raqso/search-it",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node dist/back/server.js",
    "client": "concurrently \"webpack --mode development --watch\" \"webpack-dev-server --watch --mode development --devtool inline-source-map --hot\"",
    "server": "nodemon dist/back/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "webpack-dev-server --watch --mode development --devtool inline-source-map --hot",
    "test2": "webpack --mode development --watch"
  },
  "scripts-info": {
    "build": "Build for production",
    "start": "Start production server",
    "client": "Start development client side only",
    "server": "Start development server side only",
    "dev": "Start development server"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "@types/node-cron": "^2.0.0",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.10",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "google-closure-compiler": "^20181028.0.1",
    "mongoose": "^5.3.13",
    "node-cron": "^2.0.3",
    "puppeteer": "^1.10.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "sha.js": "^2.4.11",
    "vscode": "^1.1.22"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/mongoose": "^5.2.19",
    "@types/node": "^10.12.0",
    "@types/puppeteer": "^1.9.0",
    "ava": "1.0.0-beta.7",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^1.0.0",
    "codecov": "^3.1.0",
    "concurrently": "^4.0.0",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "file-loader": "^2.0.0",
    "gh-pages": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.0.1",
    "opn-cli": "^4.0.0",
    "prettier": "^1.14.3",
    "source-map-loader": "^0.2.4",
    "standard-version": "^4.4.0",
    "style-loader": "^0.23.0",
    "trash-cli": "^1.4.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-immutable": "^5.0.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-node-externals": "^1.7.2"
  },
  "ava": {
    "failFast": true,
    "files": [
      "build/main/**/*.spec.js"
    ],
    "sources": [
      "build/main/**/*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
