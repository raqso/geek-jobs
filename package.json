{
  "name": "geek-jobs",
  "version": "1.0.0",
  "description": "IT Job searcher",
  "main": "dist/back/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/raqso/search-it",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "webpack --mode production",
    "build:client": "yarn build --config-name client",
    "build:server": "yarn build --config-name backend",
    "heroku-postbuild": "yarn build:server",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "cron": "pm2 start dist/back/cron.js",
    "download": "ts-node src/back/jobs-scrapper/scripts/downloadOffers.ts",
    "client": "webpack-dev-server --watch --mode development --devtool inline-source-map --hot --config-name client",
    "server": "concurrently \"webpack --mode development --watch --config-name backend\" \"nodemon dist/back/server.js\"",
    "dev": "concurrently \"yarn run server\" \"yarn run client\""
  },
  "scripts-info": {
    "build": "Build for production",
    "start": "Start production server",
    "client": "Start development client side only",
    "server": "Start development server side only",
    "dev": "Start development server"
  },
  "engines": {
    "node": "^14"
  },
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/styles": "^4.1.1",
    "@types/cheerio": "^0.22.11",
    "@types/node-cron": "^2.0.2",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "@types/request": "^2.48.1",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.7",
    "node-cron": "^2.0.3",
    "pm2": "^4.5.6",
    "puppeteer": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/mongoose": "^5.5.6",
    "@types/node": "^12.0.8",
    "@types/puppeteer": "^1.12.4",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.0",
    "css-loader": "^3.0.0",
    "dotenv-webpack": "^7.0.2",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.0",
    "typescript": "^3.5.2",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-node-externals": "^1.7.2"
  },
  "prettier": {
    "singleQuote": true
  }
}
