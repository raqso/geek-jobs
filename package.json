{
  "name": "search-it",
  "version": "1.0.0",
  "description": "IT Job search",
  "main": "dist/back/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/raqso/search-it",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "webpack --mode production",
    "start": "yarn run build && pm2 start dist/back/server.js",
    "cron": "pm2 start dist/back/cron.js",
    "client": "concurrently \"webpack --mode development --watch\" \"webpack-dev-server --watch --mode development --devtool inline-source-map --hot\"",
    "server": "nodemon dist/back/server.js",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "test": "yarn build"
  },
  "scripts-info": {
    "build": "Build for production",
    "start": "Start production server",
    "client": "Start development client side only",
    "server": "Start development server side only",
    "dev": "Start development server"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/styles": "^4.1.1",
    "@types/cheerio": "^0.22.11",
    "@types/jss": "^9.5.8",
    "@types/node-cron": "^2.0.2",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-router-dom": "^4.3.4",
    "@types/request": "^2.48.1",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "google-closure-compiler": "^20190528.0.0",
    "jss": "^9.8.7",
    "mongoose": "^5.6.0",
    "node-cron": "^2.0.3",
    "puppeteer": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "request": "^2.88.0",
    "sha.js": "^2.4.11",
    "vscode": "^1.1.34"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/mongoose": "^5.5.6",
    "@types/node": "^12.0.8",
    "@types/puppeteer": "^1.12.4",
    "ava": "^2.1.0",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "codecov": "^3.5.0",
    "concurrently": "^4.1.0",
    "css-loader": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^4.0.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "opn-cli": "^5.0.0",
    "prettier": "^1.18.2",
    "source-map-loader": "^0.2.4",
    "standard-version": "^6.0.1",
    "style-loader": "^0.23.1",
    "terser": "^4.0.0",
    "terser-webpack-plugin": "^1.3.0",
    "trash-cli": "^3.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.2",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-node-externals": "^1.7.2"
  },
  "ava": {
    "failFast": true,
    "files": [
      "build/main/**/*.spec.js"
    ],
    "sources": [
      "build/main/**/*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
